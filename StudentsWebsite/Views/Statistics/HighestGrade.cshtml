@model IEnumerable<StudentsWebsite.Models.Student>

@{
    ViewBag.Title = "HighestGrade";
    Layout = "~/Views/Shared/Layout.cshtml";
}

<h2>Students with grade above average</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.FullName)
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => (item.FullName))
            </td>
        </tr>
    }

</table>